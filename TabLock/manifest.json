{
  "manifest_version": 3,
  "name": "Focus Tab Blocker",
  "version": "1.0",
  "description": "Stay focused by blocking distracting tabs.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["storage", "tabs"]
}
